SELECT pg_ddl_script('pg_ddl_oid_info(oid)'::regprocedure);
                                                               pg_ddl_script                                                                
--------------------------------------------------------------------------------------------------------------------------------------------
 --                                                                                                                                        +
 -- Name: pg_ddl_oid_info(oid); Type: FUNCTION; Schema: pg_catalog; Owner: root                                                            +
 --                                                                                                                                        +
                                                                                                                                           +
 CREATE OR REPLACE FUNCTION pg_catalog.pg_ddl_oid_info(oid, OUT oid oid, OUT name name, OUT namespace name, OUT kind text, OUT owner name) +
   RETURNS record                                                                                                                          +
   VOLATILE                                                                                                                                +
   STRICT                                                                                                                                  +
   LANGUAGE 'sql'                                                                                                                          +
   SECURITY INVOKER                                                                                                                        +
   AS $ddl$                                                                                                                                +
   SELECT c.oid,                                                                                                                           +
          c.relname AS name,                                                                                                               +
          n.nspname AS namespace,                                                                                                          +
          coalesce(tt.column2,c.relkind::text) AS kind,                                                                                    +
          pg_get_userbyid(c.relowner) AS owner                                                                                             +
     FROM pg_class c JOIN pg_namespace n ON (n.oid=c.relnamespace)                                                                         +
     left join (                                                                                                                           +
        values ('r','TABLE'),                                                                                                              +
               ('v','VIEW'),                                                                                                               +
               ('i','INDEX'),                                                                                                              +
               ('S','SEQUENCE'),                                                                                                           +
               ('s','SPECIAL'),                                                                                                            +
               ('m','MATERIALIZED VIEW'),                                                                                                  +
               ('c','TYPE'),                                                                                                               +
               ('t','TOAST'),                                                                                                              +
               ('f','FOREIGN TABLE')                                                                                                       +
     ) as tt on tt.column1 = c.relkind                                                                                                     +
    WHERE c.oid = $1                                                                                                                       +
 $ddl$;                                                                                                                                    +
                                                                                                                                           +
 ALTER FUNCTION pg_ddl_oid_info(oid) OWNER TO root;                                                                                        +
 REVOKE ALL ON FUNCTION pg_ddl_oid_info(oid) FROM PUBLIC;                                                                                  +
                                                                                                                                           +
 
(1 row)

